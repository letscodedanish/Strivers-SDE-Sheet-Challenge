#include <bits/stdc++.h>

pair<int,int> missingAndRepeating(vector<int> &arr, int n)
{
	// Write your code here 
	pair<int,int> ans;
	sort(arr.begin(),arr.end());

	for(int i=0;i<n-1;i++){
         if(arr[i]==arr[i+1]){
             ans.second=(arr[i]);
	    }
    }
	if(arr[0]!=1){
        ans.first=1;
        return ans;
    }
    if(arr[n-1]!=n){
        ans.first=n;
        return ans;
    }
    for(int i=1;i<n;i++){
        if(arr[i]-arr[i-1]==2){
            ans.first=(arr[i-1]+1);
        }
    }
	

	return ans;
	
}
